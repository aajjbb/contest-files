N, M = map(int, raw_input())

state_b = [ 0 for i in range(N) ]
state_g = [ 0 for i in range(M) ]

B = int(raw_input())
already_b = list(map(int, raw_input().split(' ')))
   
G = int(raw_input())
already_g = list(map(int, raw_input().split(' ')))

for i in range(N + M):
    pos_b = i % N
    pos_g = i % M

    if state_b[pos_b] == 1 or state_g[pos_g] == 1:
        state_b[pos_b] = 1
        state_g[pos_g] = 1
